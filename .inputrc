set completion-ignore-case on
set visible-stats on
set show-all-if-ambiguous on
set expand-tilde on
set bell-style none
set history-preserve-point

"\e[A": history-search-backward
"\e[B": history-search-forward
"\e[C": forward-char
"\e[D": backward-char
"\e[3~": delete-char
"\e[7~": beginning-of-line
"\e[8~": end-of-line
"\e[1;5C": forward-word
"\e[1;5D": backward-word
"\e[5C": forward-word
"\e[5D": backward-word
"\e\e[C": forward-word
"\e\e[D": backward-word
